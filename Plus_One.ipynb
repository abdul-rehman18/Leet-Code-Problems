   "source": [
    "def plusOne(digits):\n",
    "    s= int(''.join(map(str,digits)))+1\n",
    "    m_l = list(map(int,str(s)))\n",
    "    return m_l\n",
    "    \n",
    "digits = [9]\n",
    "ans=plusOne(digits)\n",
    "print(ans)"
   ]

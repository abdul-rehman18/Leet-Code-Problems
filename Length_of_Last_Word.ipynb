   "source": [
    "def lengthOfLastWord(s):\n",
    "    m_l = s.strip()\n",
    "    return len(m_l.split(\" \")[-1])\n",
    "    \n",
    "s = \"   fly me   to   the moon  \"\n",
    "lengthOfLastWord(s)"
   ]

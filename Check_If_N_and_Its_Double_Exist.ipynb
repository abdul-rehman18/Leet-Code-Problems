      ],
      "source": [
        "def checkIfExist(arr):\n",
        "        s = set()\n",
        "        for i in arr:\n",
        "            if i*2 in s or i/2 in s:\n",
        "                return True\n",
        "            s.add(i)\n",
        "        return False\n",
        "\n",
        "arr = [10,2,5,3]\n",
        "print(checkIfExist(arr))"
      ]

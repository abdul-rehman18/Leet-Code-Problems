      "source": [
        "def toLowerCase(s):\n",
        "        a=\"\"\n",
        "        for i in s:\n",
        "            if 64<ord(i)<91:\n",
        "                i=chr(ord(i)+32)\n",
        "                a+=i\n",
        "            else:\n",
        "                a+=i\n",
        "        return a\n",
        "\n",
        "s=\"HeLLo\"\n",
        "print(toLowerCase(s))"
      ]

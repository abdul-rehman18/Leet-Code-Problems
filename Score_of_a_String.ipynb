      ],
      "source": [
        "def scoreOfString(s):\n",
        "        sc = 0\n",
        "        for i in range(len(s)-1):\n",
        "            sc += abs(ord(s[i])-ord(s[i+1]))\n",
        "        return sc\n",
        "\n",
        "s = \"hello\"\n",
        "print(scoreOfString(s))"
      ]

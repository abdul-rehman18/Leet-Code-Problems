      "source": [
        "def repeatedSubstringPattern(s):\n",
        "    concatenated = s + s\n",
        "    # print(concatenated)\n",
        "    concatenated = concatenated[1:-1]\n",
        "    # print(concatenated)\n",
        "    return s in concatenated\n",
        "\n",
        "s = \"aba\"\n",
        "print(repeatedSubstringPattern(s))\n"
      ]

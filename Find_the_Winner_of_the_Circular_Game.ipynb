      "source": [
        "def findTheWinner(n, k):\n",
        "        return (findTheWinner(n-1, k)+(k-1))%n+1 if n>1 else 1\n",
        "\n",
        "\n",
        "n = 5\n",
        "k = 2\n",
        "\n",
        "print(findTheWinner(n, k))"

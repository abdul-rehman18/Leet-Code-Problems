      "source": [
        "def firstMissingPositive(nums):\n",
        "        nums = set(nums)\n",
        "        i = 1\n",
        "        while i in nums:\n",
        "            i += 1\n",
        "        return i\n",
        "\n",
        "nums = [3,4,-1,1]\n",
        "print(firstMissingPositive(nums))"
      ]

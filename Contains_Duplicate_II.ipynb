        "def containsNearbyDuplicate(nums, k):\n",
        "  h ={}\n",
        "  for i,n in enumerate(nums):\n",
        "    if n in h and abs(i-h[n])<=k:\n",
        "      return True\n",
        "    else:\n",
        "      h[n]=i\n",
        "  return False\n",
        "\n",
        "nums = [1,2,3,1]\n",
        "k = 3\n",
        "print(containsNearbyDuplicate(nums,k))"
      ]

   "source": [
    "def isPalindrome(x):\n",
    "    if x<0:\n",
    "        return False\n",
    "    rev=0\n",
    "    temp=x\n",
    "    while temp != 0:\n",
    "        digit = temp % 10\n",
    "        rev = rev * 10 + digit\n",
    "        temp //= 10\n",
    "\n",
    "    return rev == x\n",
    "isPalindrome(121)"
   ]

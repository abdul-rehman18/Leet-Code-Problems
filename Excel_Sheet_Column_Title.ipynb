      "source": [
        "def convertToTitle(columnNumber):\n",
        "        if not columnNumber:\n",
        "            return \"\"\n",
        "        columnNumber,remainder = divmod(columnNumber-1,26)\n",
        "        return convertToTitle(columnNumber)+ chr(65+ remainder)\n",
        "\n",
        "\n",
        "convertToTitle(25)"
      ]

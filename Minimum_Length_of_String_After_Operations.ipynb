      "source": [
        "import numpy as np\n",
        "from collections import Counter\n",
        "def minimumLength(s):\n",
        "        return sum(1 if x % 2 else 2 for x in Counter(s).values())\n",
        "\n",
        "s = \"abaacbcbb\"\n",
        "\n",
        "\n",
        "print(minimumLength(s))"
      ]

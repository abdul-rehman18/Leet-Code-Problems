      "source": [
        "from math import sqrt\n",
        "def judgeSquareSum(c):\n",
        "  for a in range(int(sqrt(c)) + 1):\n",
        "    b = sqrt(c - a * a)\n",
        "    if b == int(b):\n",
        "      return True\n",
        "  return False\n",
        "\n",
        "c = 5\n",
        "print(judgeSquareSum(c))"
      ]

      "source": [
        "def countBits(n):\n",
        "        counter = [0]\n",
        "        for i in range(1 , n+1):\n",
        "            counter.append(counter[i >> 1] + i % 2)\n",
        "        return counter\n",
        "\n",
        "n = 2\n",
        "res = countBits(n)\n",
        "print(res)"
      ]

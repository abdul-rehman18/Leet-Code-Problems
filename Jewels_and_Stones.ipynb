        "def numJewelsInStones(jewels,stones):\n",
        "        return sum(i in jewels for i in stones)\n",
        "\n",
        "jewels = \"aA\"\n",
        "stones = \"aAAbbbb\"\n",
        "\n",
        "print(numJewelsInStones(jewels,stones))"

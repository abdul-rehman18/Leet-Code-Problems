      "source": [
        "def detectCapitalUse(word):\n",
        "        c = sum(w.isupper() for w in word)\n",
        "\n",
        "        return c == len(word) or c == 0 or c == 1 and word[0].isupper()\n",
        "\n",
        "\n",
        "word = \"USA\"\n",
        "print(detectCapitalUse(word))"

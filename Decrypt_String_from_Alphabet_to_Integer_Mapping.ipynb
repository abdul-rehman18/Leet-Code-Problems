        "# Decrypt String from Alphabet to Integer Mapping"
      ],
      "metadata": {
        "id": "PHfXjq3JdfUA"
      }
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "U8kYt_Nhc45J"
      },
      "outputs": [],
      "source": [
        "class Solution:\n",
        "    def freqAlphabets(self, s: str) -> str:\n",
        "        for i in range(26,0,-1): s = s.replace(str(i)+'#'*(i>9),chr(96+i))\n",
        "        return s\n"
      ]
    }
  ]
}
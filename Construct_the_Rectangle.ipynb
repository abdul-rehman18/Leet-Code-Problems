          "output_type": "stream",
          "name": "stdout",
          "text": [
            "[2, 2]\n"
          ]
        }
      ],
      "source": [
        "def constructRectangle(area):\n",
        "        for i in range(int(area**0.5),0,-1):\n",
        "            if area % i == 0:\n",
        "                return [area//i,i]\n",
        "\n",
        "area = 4\n",
        "res = constructRectangle(area)\n",
        "print(res)"
      ]

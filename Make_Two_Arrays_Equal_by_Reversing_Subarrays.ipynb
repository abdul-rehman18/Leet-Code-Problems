      ],
      "source": [
        "from collections import Counter\n",
        "\n",
        "def canBeEqual(target, arr):\n",
        "        return Counter(target) == Counter(arr)\n",
        "\n",
        "\n",
        "target = [1,2,3,4]\n",
        "arr = [2,4,1,3]\n",
        "\n",
        "print(canBeEqual(target, arr))\n"
      ]

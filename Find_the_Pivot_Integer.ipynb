      "source": [
        "import math\n",
        "def pivotInteger(n):\n",
        "        ans = math.sqrt(n * (n + 1) / 2)\n",
        "        if ans % 1 == 0:\n",
        "            return int(ans)\n",
        "        return -1\n",
        "\n",
        "\n",
        "n = 8\n",
        "print(pivotInteger(n))"
      ]

      "source": [
        "def isOneBitCharacter(bits):\n",
        "        ret = True\n",
        "        for bit in bits[-2::-1]:\n",
        "            if bit: ret = not ret\n",
        "            else: break\n",
        "        return ret\n",
        "bits = [1,0,0]\n",
        "print(isOneBitCharacter(bits))"
      ]

   "source": [
    "def removeElement(nums,val):\n",
    "    nums[:] = [num for num in nums if val!=num]\n",
    "    return len(nums)\n",
    "    \n",
    "len_list=removeElement([0,1,2,2,3,0,4,2],2)\n",
    "print(len_list)"
   ]

        }
      ],
      "source": [
        "def smallestRangeI(nums, k):\n",
        "        l= min(nums)+k\n",
        "        r= max(max(nums)-k,l)\n",
        "\n",
        "        return r-l\n",
        "\n",
        "nums=[0,10]\n",
        "k=2\n",
        "\n",
        "\n",
        "print(smallestRangeI(nums, k))"
      ]

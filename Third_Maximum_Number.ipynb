        "def thirdMax(nums):\n",
        "        nums = sorted(list(set(nums)))\n",
        "        if len(nums)>2:\n",
        "            return nums[-3]\n",
        "        return nums[-1]\n",
        "\n",
        "nums = [3,2,1]\n",

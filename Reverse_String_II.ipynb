      "source": [
        "def reverseStr(s, k):\n",
        "        s=list(s)\n",
        "        for i in range(0,len(s),2*k):\n",
        "            s[i:i+k]=(s[i:i+k])[::-1]\n",
        "        return ''.join(s)\n",
        "\n",
        "\n",
        "s = \"abcdefg\"\n",
        "k = 2\n",
        "print(reverseStr(s, k))"
      ]

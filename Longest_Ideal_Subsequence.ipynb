      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "4\n"
          ]
        }
      ],
      "source": [
        "def longestIdealString(s, k):\n",
        "        l = [0] * 128\n",
        "        for c in s:\n",
        "            i = ord(c)\n",
        "            l[i] = max(l[i - k : i + k + 1]) + 1\n",
        "        return max(l)\n",
        "\n",
        "\n",
        "\n",
        "s = \"acfgbd\"\n",
        "k = 2\n",
        "print(longestIdealString(s, k))"
      ]
    },

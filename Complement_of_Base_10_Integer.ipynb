      "source": [
        "def bitwiseComplement(n):\n",
        "        if n == 0:\n",
        "            return 1\n",
        "        bits = n.bit_length()\n",
        "        mask = (1 << bits) - 1\n",
        "        return n ^ mask\n",
        "\n",
        "\n",
        "n = 5\n",
        "print(bitwiseComplement(n))"
      ]

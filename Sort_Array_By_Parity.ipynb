      "source": [
        "def sortArrayByParity(nums):\n",
        "        return [i for i in nums if i%2==0]+[i for i in nums if i%2==1]\n",
        "\n",
        "\n",
        "nums=[3,1,2,4]\n",
        "print(sortArrayByParity(nums))"
      ]

        "outputId": "7fb68037-15ec-4c0d-cf43-4ca7e93be41d"
      },
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "3\n"
          ]
        }
      ],
      "source": [
        "def repeatedNTimes(nums):\n",
        "        c = set()\n",
        "        for i in nums:\n",
        "            if i in c:\n",
        "                return i\n",
        "            c.add(i)\n",
        "        return 0\n",
        "\n",
        "nums = [1,2,3,3]\n",
        "print(repeatedNTimes(nums))"
      ]

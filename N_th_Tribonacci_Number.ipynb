      "source": [
        "def tribonacci(n):\n",
        "  if n == 0:\n",
        "    return 0\n",
        "  if n == 1 or n == 2:\n",
        "    return 1\n",
        "\n",
        "  a,b,c = 0,1,1\n",
        "  for _ in range(3,n+1):\n",
        "    ne = a+b+c\n",
        "    a,b,c = b,c,ne\n",
        "  return c\n",
        "\n",
        "n = 25\n",
        "print(tribonacci(n))"
      ]

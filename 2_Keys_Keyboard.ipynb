      "source": [
        "def minSteps(n):\n",
        "        if n == 1:\n",
        "            return 0\n",
        "        s = 0\n",
        "        f = 2\n",
        "        while n > 1:\n",
        "            while n % f == 0:\n",
        "                s += f\n",
        "                n //= f\n",
        "            f += 1\n",
        "        return s\n",
        "\n",
        "n = 3\n",
        "\n",
        "print(minSteps(n))\n"
      ]

      "source": [
        "def distributeCandies(candyType):\n",
        "        c_l = len(candyType)//2\n",
        "        u_l = len(set(candyType))\n",
        "        return min(u_l,c_l)\n",
        "\n",
        "\n",
        "candyType = [1,1,2,2,3,3]\n",
        "print(distributeCandies(candyType))"
      ]

      },
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "aaacecaaa\n"
          ]
        }
      ],
      "source": [
        "def shortestPalindrome(s):\n",
        "        r = s[::-1]\n",
        "        for i in range(len(s) + 1):\n",
        "            if s.startswith(r[i:]):\n",
        "                return r[:i] + s\n",
        "\n",
        "s = \"aacecaaa\"\n",
        "print(shortestPalindrome(s))"
      ]
    },

      "source": [
        "def moveZeroes(nums):\n",
        "\n",
        "        i = 0\n",
        "        n = len(nums)\n",
        "        non_zero = [num for num in nums if num != 0]\n",
        "        nums[:n] = non_zero + [0] * (n - len(non_zero))\n",
        "        return nums\n",
        "\n",
        "\n",
        "nums = [0,1,0,3,12]\n",
        "res = moveZeroes(nums)\n",
        "print(res)"
      ]

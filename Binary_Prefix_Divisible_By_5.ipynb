      "source": [
        "def prefixesDivBy5(nums):\n",
        "        ans = []\n",
        "        curr = 0\n",
        "        for num in nums:\n",
        "            curr = (curr * 2 + num) % 5\n",
        "            ans.append(curr == 0)\n",
        "        return ans\n",
        "\n",
        "nums = [0,1,1]\n",
        "print(prefixesDivBy5(nums))"
      ]

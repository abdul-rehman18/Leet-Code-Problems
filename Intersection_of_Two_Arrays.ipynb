      "source": [
        "def intersection(nums1, nums2):\n",
        "  n1 = set(nums1)\n",
        "  n2 = set(nums2)\n",
        "  return list(n1.intersection(n2))\n",
        "\n",
        "# nums1 = [1,2,2,1]\n",
        "# nums2 = [2,2]\n",
        "nums1 = [4,9,5]\n",
        "nums2 = [9,4,9,8,4]\n",
        "\n",
        "res = intersection(nums1,nums2)\n",
        "print(res)"
      ]
